<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".FinalAwarenessActivity" >

    <TextView
        android:id="@+id/level_00_finish_06_text_01"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:text="@string/level_00_finish_06_text_01"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <ImageView
        android:id="@+id/level_00_finish_06_image_01"
        android:layout_width="@dimen/icon_book_write"
        android:layout_height="@dimen/icon_book_write"
        android:layout_alignLeft="@+id/level_00_finish_06_text_01"
        android:layout_below="@+id/level_00_finish_06_text_01"
        android:layout_centerVertical="true"
        android:layout_marginTop="@dimen/margin_standard"
        android:src="@drawable/emblem_library" />

    <TextView
        android:id="@+id/level_00_finish_06_text_02"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/level_00_finish_06_image_01"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="@dimen/margin_standard"
        android:paddingLeft="@dimen/margin_small"
        android:text="@string/level_00_finish_06_text_02"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <ImageView
        android:id="@+id/level_00_finish_06_image_02"
        android:layout_width="@dimen/icon_book_write"
        android:layout_height="@dimen/icon_book_write"
        android:layout_alignLeft="@+id/level_00_finish_06_image_01"
        android:layout_below="@+id/level_00_finish_06_image_01"
        android:layout_marginTop="@dimen/margin_standard"
        android:src="@drawable/desktop" />

    <TextView
        android:id="@+id/level_00_finish_06_text_03"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/level_00_finish_06_text_02"
        android:layout_alignTop="@+id/level_00_finish_06_image_02"
        android:paddingLeft="@dimen/margin_small"
        android:text="@string/level_00_finish_06_text_03"
        android:textAppearance="?android:attr/textAppearanceMedium" />

</RelativeLayout>