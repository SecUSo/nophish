<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin" >

   <ImageView
        android:id="@+id/level_2_web_address_img_checkmark_5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/margin_standard"
        android:src="@drawable/checkmark" />

    <TextView
        android:id="@+id/level_2_example_amazon_4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/level_2_web_address_img_checkmark_5"
        android:layout_toRightOf="@+id/level_2_web_address_img_checkmark_5"
        android:text="@string/level_2_example_amazon_http_and_after"
        android:textAppearance="?android:attr/textAppearanceMedium" />

     <TextView
         android:id="@+id/level_2_example_amazon_5"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_alignBaseline="@+id/level_2_example_amazon_4"
         android:layout_alignBottom="@+id/level_2_example_amazon_4"
         android:layout_toRightOf="@+id/level_2_example_amazon_4"
         android:background="@drawable/background_highlight"
         android:padding="@dimen/margin_xxsmall"
         android:text="@string/level_2_example_amazon_domain"
         android:textAppearance="?android:attr/textAppearanceMedium" />
    
     <ImageView
        android:id="@+id/level_2_web_address_img_cross_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/level_2_web_address_img_checkmark_5"
    	android:layout_marginTop="@dimen/margin_standard"
        android:src="@drawable/cross"
        android:visibility="visible" />

    <TextView
        android:id="@+id/level_2_example_amazon_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/level_2_example_amazon_fake"
        android:layout_alignBottom="@+id/level_2_example_amazon_fake"
        android:layout_toRightOf="@+id/level_2_example_amazon_fake"
        android:text="@string/level_2_example_amazon_fake_domain"
        android:background="@drawable/background_highlight"
        android:padding="@dimen/margin_xxsmall"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/level_2_example_amazon_fake"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/level_2_example_amazon_4"
        android:layout_alignTop="@+id/level_2_web_address_img_cross_2"
        android:text="@string/level_2_example_amazon_fake_http"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/level_2_example_amazon_fake_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/level_2_example_amazon_fake"
        android:layout_alignLeft="@+id/level_2_example_amazon_fake"
        android:text="@string/level_2_example_amazon_fake_path"
        android:textAppearance="?android:attr/textAppearanceMedium" />
    
    
    
      <ImageView
        android:id="@+id/level_2_web_address_img_checkmark_6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/level_2_web_address_img_cross_2"
        android:layout_marginTop="@dimen/margin_standard"
        android:src="@drawable/checkmark" />

    <TextView
        android:id="@+id/level_2_google_example"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/level_2_example_amazon_fake_1"
        android:layout_alignTop="@+id/level_2_web_address_img_checkmark_6"
        android:text="@string/level_2_example_googlemail_http"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/level_2_google_example_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/level_2_google_example"
        android:layout_alignBottom="@+id/level_2_google_example"
        android:layout_toRightOf="@+id/level_2_google_example"
        android:text="@string/level_2_example_googlemail_domain"
        android:background="@drawable/background_highlight"
        android:padding="@dimen/margin_xxsmall"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/level_2_google_example_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/level_2_google_example"
        android:layout_below="@+id/level_2_google_example"
        android:text="@string/level_2_example_googlemail_path"
        android:textAppearance="?android:attr/textAppearanceMedium" />
    
    <ImageView
        android:id="@+id/level_2_web_address_img_cross_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/level_2_web_address_img_checkmark_6"
    	android:layout_marginTop="@dimen/margin_standard"
        android:src="@drawable/cross"
        android:visibility="visible" />
    
    
    <TextView
        android:id="@+id/level_2_google_fake_example"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/level_2_example_amazon_fake_1"
        android:layout_alignTop="@+id/level_2_web_address_img_cross_3"
        android:text="@string/level_2_example_googlemail_fake_http"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/level_2_google_fake_example_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/level_2_google_fake_example"
        android:layout_alignBottom="@+id/level_2_google_fake_example"
        android:layout_toRightOf="@+id/level_2_google_fake_example"
        android:text="@string/level_2_example_googlemail_fake_domain"
        android:background="@drawable/background_highlight"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/level_2_goole_fake_example_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/level_2_google_fake_example_2"
        android:layout_alignBottom="@+id/level_2_google_fake_example_2"
        android:layout_toRightOf="@+id/level_2_google_fake_example_2"
        android:text="@string/level_2_example_googlemail_fake_path"
        android:textAppearance="?android:attr/textAppearanceMedium" />
    
    
    <ImageView
        android:id="@+id/level_2_web_address_img_cross_4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/level_2_web_address_img_cross_3"
    	android:layout_marginTop="@dimen/margin_standard"
        android:src="@drawable/cross"
        android:visibility="invisible" />

    
    <TextView
        android:id="@+id/level_2_facebook_fake_example"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/level_2_example_amazon_fake_1"
        android:layout_alignTop="@+id/level_2_web_address_img_cross_4"
        android:text="@string/level_2_example_facebook_fake"
        android:textAppearance="?android:attr/textAppearanceMedium" />
</RelativeLayout>