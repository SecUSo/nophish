<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/level_07_intro_06_sv"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fadeScrollbars="false" >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin" >

        <TextView
            android:id="@+id/level_07_intro_06_text_01"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:text="@string/level_07_intro_06_text_01"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <TextView
            android:id="@+id/level_07_intro_06_text_02"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/level_07_intro_06_text_01"
            android:layout_marginTop="@dimen/margin_standard"
            android:text="@string/level_07_intro_06_text_02"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <TextView
            android:id="@+id/level_07_intro_06_http_01"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/level_07_intro_06_text_02"
            android:layout_marginTop="@dimen/margin_xxsmall"
            android:text="@string/http"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <TextView
            android:id="@+id/level_07_intro_06_www_01"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/level_07_intro_06_http_01"
            android:layout_alignBottom="@+id/level_07_intro_06_http_01"
            android:layout_toRightOf="@+id/level_07_intro_06_http_01"
            android:text="@string/www"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <TextView
            android:id="@+id/level_07_intro_06_facebook_login"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/level_07_intro_06_www_01"
            android:layout_alignBottom="@+id/level_07_intro_06_www_01"
            android:layout_toRightOf="@+id/level_07_intro_06_www_01"
            android:background="@color/red"
            android:text="@string/level_07_intro_03_facebook_login"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <TextView
            android:id="@+id/level_07_intro_06_path_01"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/level_07_intro_06_facebook_login"
            android:layout_alignBottom="@+id/level_07_intro_06_facebook_login"
            android:layout_toRightOf="@+id/level_07_intro_06_facebook_login"
            android:text="@string/slash"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <TextView
            android:id="@+id/level_07_intro_06_text_03"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/level_07_intro_06_http_01"
            android:layout_marginTop="@dimen/margin_standard"
            android:text="@string/level_07_intro_06_text_03"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <TextView
            android:id="@+id/level_07_intro_06_http_02"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/level_07_intro_06_text_03"
            android:layout_marginTop="@dimen/margin_xxsmall"
            android:text="@string/https"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <TextView
            android:id="@+id/level_07_intro_06_www_02"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/level_07_intro_06_http_02"
            android:layout_alignBottom="@+id/level_07_intro_06_http_02"
            android:layout_toRightOf="@+id/level_07_intro_06_http_02"
            android:text="@string/www"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <TextView
            android:id="@+id/level_07_intro_06_fracebook"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/level_07_intro_06_www_02"
            android:layout_alignBottom="@+id/level_07_intro_06_www_02"
            android:layout_toRightOf="@+id/level_07_intro_06_www_02"
            android:background="@color/red"
            android:text="@string/level_07_intro_04_fracebook"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <TextView
            android:id="@+id/level_07_intro_06_path_02"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/level_07_intro_06_fracebook"
            android:layout_alignBottom="@+id/level_07_intro_06_fracebook"
            android:layout_toRightOf="@+id/level_07_intro_06_fracebook"
            android:text="@string/level_07_intro_04_path"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <TextView
            android:id="@+id/level_07_intro_06_text_04"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/level_07_intro_06_http_02"
            android:layout_marginTop="@dimen/margin_standard"
            android:text="@string/level_07_intro_06_text_04"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <TextView
            android:id="@+id/level_07_intro_06_http_03"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/level_07_intro_06_text_04"
            android:layout_marginTop="@dimen/margin_xxsmall"
            android:text="@string/http"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <TextView
            android:id="@+id/level_07_intro_06_www_03"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/level_07_intro_06_http_03"
            android:layout_alignBottom="@+id/level_07_intro_06_http_03"
            android:layout_toRightOf="@+id/level_07_intro_06_http_03"
            android:text="@string/www"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <TextView
            android:id="@+id/level_07_intro_06_mircosoft"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/level_07_intro_06_www_03"
            android:layout_alignBottom="@+id/level_07_intro_06_www_03"
            android:layout_toRightOf="@+id/level_07_intro_06_www_03"
            android:background="@color/red"
            android:text="@string/level_07_intro_05_mircosoft"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <TextView
            android:id="@+id/level_07_intro_06_path_03"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/level_07_intro_06_mircosoft"
            android:layout_alignBottom="@+id/level_07_intro_06_mircosoft"
            android:layout_toRightOf="@+id/level_07_intro_06_mircosoft"
            android:text="@string/level_07_intro_05_path_01"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <TextView
            android:id="@+id/level_07_intro_06_path_04"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/level_07_intro_06_http_03"
            android:text="@string/level_07_intro_05_path_02"
            android:textAppearance="?android:attr/textAppearanceMedium" />
    </RelativeLayout>

</ScrollView>