    <string name="game_intro_title">Einführung</string>
    <string name="game_intro_text_2">Das Spiel umfasst verschiedene Levels. Jedem Level ist jeweils eine kurze Lektion vorangestellt.</string>
    <string name="game_intro_text_describe_lib_emblem">Einführung und Erklärung</string>
